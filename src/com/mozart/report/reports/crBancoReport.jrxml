	<queryString>
		<![CDATA[SELECT 1 TIPO, B.NOME_FANTASIA BANCO, DU.ID_DUPLICATA, ER.NOME_FANTASIA,
RTRIM(LTRIM(DU.NUM_DUPLICATA || '-' || DU.NUM_PARCELAS || '-' || SUBSTR(DU.ANO,3,2))) AS DUP,
DU.VALOR_DUPLICATA, DU.DATA_VENCIMENTO, DU.COMISSAO, DU.IR, DU.AJUSTES,
DU.ENCARGOS, DU.ID_NOTA, DU.DATA_DESCONTO,
DU.VALOR_DUPLICATA - NVL(DU.ENCARGOS, 0) - NVL(DU.COMISSAO, 0) + NVL(DU.IR, 0) - NVL(DU.AJUSTES, 0) AS VALOR_LIQUIDO,DU.DATA_LANCAMENTO, DU.DATA_RECEBIMENTO
FROM DUPLICATA DU
JOIN HOTEL H ON (DU.ID_HOTEL = H.ID_HOTEL)
JOIN EMPRESA_REDE ER ON (H.ID_REDE_HOTEL = ER.ID_REDE_HOTEL)
JOIN EMPRESA_HOTEL EM ON (DU.ID_HOTEL = EM.ID_HOTEL AND DU.ID_EMPRESA = EM.ID_EMPRESA AND EM.ID_EMPRESA = ER.ID_EMPRESA)
JOIN CONTA_CORRENTE CC ON ( DU.ID_CONTA_CORRENTE = CC.ID_CONTA_CORRENTE)
JOIN BANCO B ON (CC.ID_BANCO = B.ID_BANCO)
WHERE DU.ID_HOTEL = $P{IDH}
AND B.ID_BANCO = $P{P_BANCO}
AND DU.DATA_VENCIMENTO >= to_Date($P{P_DATA},'dd/mm/yyyy')
AND DU.DATA_VENCIMENTO <= to_Date($P{P_DATA_FIM},'dd/mm/yyyy')
ORDER BY ANO, DUP]]>
	</queryString>
