	<field name="JUROS_MES" class="java.math.BigDecimal"/>
	<field name="DESCONTO_MES" class="java.math.BigDecimal"/>
	<field name="IMPOSTOS_MES" class="java.math.BigDecimal"/>
	<field name="TOTAL_LIQUIDO_MES" class="java.math.BigDecimal"/>
	<field name="FAT_DIA" class="java.math.BigDecimal"/>
	<field name="FAT_MES" class="java.math.BigDecimal"/>
	<variable name="TOTAL_VR_RECEBIDO" class="java.lang.Double" resetType="Group" resetGroup="GROUP_CC" calculation="Sum">
		<variableExpression><![CDATA[$F{VALREC}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0)]]></initialValueExpression>
	</variable>
	<variable name="TOTAL_VR_LIQUIDO" class="java.lang.Double" resetType="Group" resetGroup="GROUP_CC" calculation="Sum">
		<variableExpression><![CDATA[$F{VALOR_DUPLICATA_CALCULADO}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0)]]></initialValueExpression>
	</variable>
	<variable name="TOTAL_MES_DUPLICATAS" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{TOTAL_MES}+$F{SALDO_ANTERIOR_CONTAS_RECEBER}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0)]]></initialValueExpression>
	</variable>
	<variable name="TOTAL_IMPOSTO" class="java.lang.Double" resetType="Group" resetGroup="GROUP_CC" calculation="Sum">
		<variableExpression><![CDATA[$F{IMPOSTOS}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0)]]></initialValueExpression>
	</variable>
	<variable name="TOTAL_JUROS" class="java.lang.Double" resetType="Group" resetGroup="GROUP_CC" calculation="Sum">
		<variableExpression><![CDATA[$F{JUROS_RECEBIMENTO}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0)]]></initialValueExpression>
	</variable>
	<variable name="TOTAL_DESCONTO" class="java.lang.Double" resetType="Group" resetGroup="GROUP_CC" calculation="Sum">
		<variableExpression><![CDATA[$F{DESCONTO_RECEBIMENTO}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0)]]></initialValueExpression>
	</variable>
	<variable name="TOTAL_VR_RECEBIDO_BANCO" class="java.lang.Double" resetType="Group" resetGroup="Banco_group" calculation="Sum">
		<variableExpression><![CDATA[$F{VALREC}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0)]]></initialValueExpression>
	</variable>
	<variable name="TOTAL_IMPOSTO_BANCO" class="java.lang.Double" resetType="Group" resetGroup="Banco_group" calculation="Sum">
		<variableExpression><![CDATA[$F{IMPOSTOS}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0)]]></initialValueExpression>
	</variable>
	<variable name="TOTAL_JUROS_BANCO" class="java.lang.Double" resetType="Group" resetGroup="Banco_group" calculation="Sum">
		<variableExpression><![CDATA[$F{JUROS_RECEBIMENTO}]]></variableExpression>
