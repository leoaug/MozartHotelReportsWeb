	</parameter>
	<queryString>
		<![CDATA[SELECT 1 GERAL,
         ID,
         TIPO,
         NVL (VALOR_LIQUIDO, 0) VALOR_LIQUIDO,
         NVL (IMPOSTOS, 0) IMPOSTOS,
         BANCOCC,
         NOME_FANTASIA,
         DUP,
         NVL (VALREC, 0) VALREC,
         NVL (SALDO_ANTERIOR_CONTAS_RECEBER, 0) SALDO_ANTERIOR_CONTAS_RECEBER,
         NVL (VALOR_LIQUIDO_TOTAL, 0) VALOR_LIQUIDO_TOTAL,
         NVL (VALOR_DUPLICATA_CALCULADO, 0) VALOR_DUPLICATA_CALCULADO,
         NVL (JUROS_RECEBIMENTO, 0) JUROS_RECEBIMENTO,
         NVL (DESCONTO_RECEBIMENTO, 0) DESCONTO_RECEBIMENTO,
         NUM_CONTA_CORRENTE,
         ID_BANCO,
         NVL (TOTAL_MES, 0) TOTAL_MES,
         NVL (JUROS_MES, 0) JUROS_MES,
         NVL (DESCONTO_MES, 0) DESCONTO_MES,
         NVL (IMPOSTOS_MES, 0) IMPOSTOS_MES,
         NVL (TOTAL_LIQUIDO_MES, 0) TOTAL_LIQUIDO_MES,
         FAT.*
    FROM (SELECT *
            FROM (SELECT 1 ID,
                         $P{IDH} id_hotel,
                         TOTAL_MES.TOTAL TOTAL_MES,
                         TOTAL_MES.JUROS JUROS_MES,
                         TOTAL_MES.DESCONTO DESCONTO_MES,
                         TOTAL_MES.IMPOSTOS IMPOSTOS_MES,
                           TOTAL_MES.TOTAL
                         - TOTAL_MES.JUROS
                         + TOTAL_MES.DESCONTO
                         + TOTAL_MES.IMPOSTOS
                            TOTAL_LIQUIDO_MES
                    FROM (SELECT SUM (NVL (VALOR_RECEBIDO, 0)) TOTAL,
                                 SUM (NVL (JUROS_RECEBIMENTO, 0)) JUROS,
                                 SUM (NVL (DESCONTO_RECEBIMENTO, 0)) DESCONTO,
                                 SUM (
                                      NVL (ISS, 0)
                                    + NVL (COFINS, 0)
                                    + NVL (PIS, 0)
                                    + NVL (IR_RETENCAO, 0)
                                    + NVL (CSSL, 0))
                                    IMPOSTOS
                            FROM DUPLICATA
                           WHERE TRUNC (DATA_RECEBIMENTO) BETWEEN TRUNC (
                                                                     TO_DATE (
                                                                        $P{P_DATA},
                                                                        'dd/mm/yyyy'),
                                                                     'MONTH')
                                                              AND TO_DATE (
                                                                     $P{P_DATA},
                                                                     'dd/mm/yyyy')
                                 AND ID_HOTEL = $P{IDH}) TOTAL_MES) TOTAL_MES,
                 (SELECT du.id_hotel,
       DECODE (
          DATA_DESCONTO,
          NULL, 'RECEBIDAS',
          DECODE (DATA_RECOMPRA,
                  NULL, 'DESCONTADAS E RECEBIDAS',
                  'RECEBIDAS'))
          AS TIPO,
       (  NVL (DU.VALOR_DUPLICATA, 0)
        - NVL (DU.COMISSAO, 0)
        - NVL (DU.AJUSTES, 0)
        + NVL (DU.IR, 0)
        - NVL (DU.ENCARGOS, 0))
          VALOR_LIQUIDO,
       (  NVL (DU.ISS, 0)
        + NVL (DU.COFINS, 0)
        + NVL (DU.PIS, 0)
        + NVL (DU.IR_RETENCAO, 0)
        + NVL (DU.CSSL, 0))
          IMPOSTOS,
       (BA.NOME_FANTASIA || '-' || CC.NUM_CONTA_CORRENTE) AS BANCOCC,
       ER.NOME_FANTASIA,
       DU.NUM_DUPLICATA || '-' || DU.NUM_PARCELAS || '-' || DU.ANO AS DUP,
         NVL (DU.VALOR_DUPLICATA, 0)
       - NVL (DU.COMISSAO, 0)
       - NVL (DU.AJUSTES, 0)
       + NVL (DU.IR, 0)
       - NVL (DU.ENCARGOS, 0)
       - NVL (DU.ISS, 0)
       - NVL (DU.COFINS, 0)
       - NVL (DU.PIS, 0)
       - NVL (DU.IR_RETENCAO, 0)
       - NVL (DU.CSSL, 0)
       + NVL (DU.JUROS_RECEBIMENTO, 0)
       - NVL (DU.DESCONTO_RECEBIMENTO, 0)
          AS VALREC,
       VALOR_LIQUIDO_TOTAL,
         NVL (DU.VALOR_DUPLICATA, 0)
       - NVL (DU.COMISSAO, 0)
       - NVL (DU.AJUSTES, 0)
       + NVL (DU.IR, 0)
       - NVL (DU.ENCARGOS, 0)
          VALOR_DUPLICATA_CALCULADO,
       DU.JUROS_RECEBIMENTO,
       DU.DESCONTO_RECEBIMENTO,
       CC.NUM_CONTA_CORRENTE,
       CC.ID_BANCO
  FROM (SELECT SUM (
                    (NVL (DU.VALOR_DUPLICATA, 0))
                  - (NVL (DU.ENCARGOS, 0))
                  - (NVL (DU.COMISSAO, 0))
                  + (NVL (DU.IR, 0))
                  - (NVL (DU.AJUSTES, 0)))
                  AS VALOR_LIQUIDO_TOTAL
          FROM DUPLICATA DU,
               STATUS_NOTA SN,
               EMPRESA_REDE ER,
               (SELECT C.NUM_CONTA_CORRENTE,C.ID_CONTA_CORRENTE, C.ID_BANCO, C.ID_HOTEL
                  FROM    CONTA_CORRENTE C
                       JOIN
                          PLANO_CONTAS P
                       ON (C.ID_HOTEL = P.ID_HOTEL_MUTUO(+))
                 WHERE C.ID_HOTEL = $P{IDH}) CC,
               BANCO BA
         WHERE     DU.ID_HOTEL = $P{IDH}
               AND DU.ID_EMPRESA = ER.ID_EMPRESA
               AND ER.ID_REDE_HOTEL = DU.ID_REDE_HOTEL
               AND DU.SITUACAO = 'R'
               AND DU.ID_CONTA_CORRENTE = CC.ID_CONTA_CORRENTE
               AND DU.ID_NOTA = SN.ID_NOTA
               AND DU.ID_HOTEL = $P{IDH}
               AND SN.ID_HOTEL = DU.ID_HOTEL
               AND CC.ID_HOTEL = DU.ID_HOTEL
               AND CC.ID_BANCO = BA.ID_BANCO
               AND TRUNC (DU.DATA_RECEBIMENTO) =
                      TO_DATE ($P{P_DATA}, 'dd/mm/yyyy')),
       DUPLICATA DU,
       STATUS_NOTA SN,
       EMPRESA_REDE ER,
       (SELECT C.NUM_CONTA_CORRENTE,C.ID_CONTA_CORRENTE, C.ID_BANCO, C.ID_HOTEL
          FROM    CONTA_CORRENTE C
               JOIN
                  PLANO_CONTAS P
               ON (C.ID_HOTEL = P.ID_HOTEL_MUTUO(+))
         WHERE C.ID_HOTEL = $P{IDH}) CC,
       BANCO BA
 WHERE     DU.ID_HOTEL = $P{IDH}
       AND SN.ID_HOTEL = DU.ID_HOTEL
       AND CC.ID_HOTEL = DU.ID_HOTEL
       AND ER.ID_REDE_HOTEL = DU.ID_REDE_HOTEL
       AND DU.ID_EMPRESA = ER.ID_EMPRESA
       AND DU.SITUACAO = 'R'
       AND DU.ID_CONTA_CORRENTE = CC.ID_CONTA_CORRENTE
       AND DU.ID_NOTA = SN.ID_NOTA
       AND CC.ID_BANCO = BA.ID_BANCO
       AND TRUNC (DU.DATA_RECEBIMENTO) = TO_DATE ($P{P_DATA}, 'dd/mm/yyyy')
) tb
           WHERE total_mes.id_hotel = tb.id_hotel(+)
          UNION ALL
          SELECT *
            FROM (SELECT 2 ID,
                         $P{IDH} id_hotel,
                         TOTAL_MES.TOTAL TOTAL_MES,
                         TOTAL_MES.JUROS JUROS_MES,
                         TOTAL_MES.DESCONTO DESCONTO_MES,
                         TOTAL_MES.IMPOSTOS IMPOSTOS_MES,
                           TOTAL_MES.TOTAL
                         - TOTAL_MES.JUROS
                         + TOTAL_MES.DESCONTO
                         + TOTAL_MES.IMPOSTOS
                            TOTAL_LIQUIDO_MES
                    FROM (SELECT SUM (NVL (VALOR_RECEBIDO, 0)) TOTAL,
                                 SUM (NVL (JUROS_RECEBIMENTO, 0)) JUROS,
                                 SUM (NVL (DESCONTO_RECEBIMENTO, 0)) DESCONTO,
                                 SUM (
                                      NVL (ISS, 0)
                                    + NVL (COFINS, 0)
                                    + NVL (PIS, 0)
                                    + NVL (IR_RETENCAO, 0)
                                    + NVL (CSSL, 0))
                                    IMPOSTOS
                            FROM DUPLICATA
                           WHERE TRUNC (DATA_DESCONTO) BETWEEN TRUNC (
                                                                  TO_DATE (
                                                                     $P{P_DATA},
                                                                     'dd/mm/yyyy'),
                                                                  'MONTH')
                                                           AND TO_DATE (
                                                                  $P{P_DATA},
                                                                  'dd/mm/yyyy')
                                 AND ID_HOTEL = $P{IDH}) TOTAL_MES) TOTAL_MES,
                 (SELECT id_hotel,
                         TIPO,
                         VALOR_LIQUIDO,
                         VALOR_IMPOSTO,
                         BANCOCC,
                         EMPRESA,
                         DUP,
                         VALOR_RECEBIDO,
                         NULL VALOR_LIQUIDO_TOTAL,
                         VALOR_DUPLICATA_CALCULADO,
                         JUROS_RECEBIMENTO,
                         DESCONTO_RECEBIMENTO,
                         NUM_CONTA_CORRENTE,
                         ID_BANCO
                    FROM (SELECT 'Descontada' TIPO,
                                 dup.id_hotel,
                                 (dup.id_duplicata),
                                    dup.num_duplicata
                                 || '-'
                                 || dup.num_parcelas
                                 || '-'
                                 || dup.ano
                                    AS dup,
                                 dup.id_duplicata_descontada,
                                 TRUNC (dup.data_recebimento),
                                 TRUNC (dup.data_recompra),
                                 NVL (dup.valor_recebido, 0) valor_recebido,
                                 NVL (dup.desp_financeira, 0) valor_imposto,
                                 NVL (dup.juros_recebimento, 0) valor_juros,
                                 NVL (dup.ajustes, 0) valor_desconto,
                                 emp.nome_fantasia empresa,
                                 dup.valor_duplicata,
                                   DUP.VALOR_DUPLICATA
                                 - NVL (DUP.ENCARGOS, 0)
                                 - NVL (DUP.COMISSAO, 0)
                                 + NVL (DUP.IR, 0)
                                 - NVL (DUP.AJUSTES, 0)
                                 - NVL (DUP.PIS, 0)
                                 - NVL (DUP.COFINS, 0)
                                 - NVL (DUP.ISS, 0)
                                 - NVL (DUP.IR_RETENCAO, 0)
                                 - NVL (DUP.CSSL, 0)
                                    AS VALOR_LIQUIDO,
                                   NVL (DUP.VALOR_DUPLICATA, 0)
                                 - NVL (DUP.COMISSAO, 0)
                                 - NVL (DUP.AJUSTES, 0)
                                 + NVL (DUP.IR, 0)
                                 - NVL (DUP.ENCARGOS, 0)
                                    VALOR_DUPLICATA_CALCULADO,
                                 TRUNC (dup.data_vencimento),
                                 TRUNC (dup.data_desconto),
                                 (   BAN.NOME_FANTASIA
                                  || '-'
                                  || CCO.NUM_CONTA_CORRENTE)
                                    AS BANCOCC,
                                 DUP.JUROS_RECEBIMENTO,
                                 DUP.DESCONTO_RECEBIMENTO,
                                 CCO.NUM_CONTA_CORRENTE,
                                 CCO.ID_BANCO
                            FROM duplicata dup
                                 JOIN duplicata_descontada dud
                                    ON (dup.ID_DUPLICATA_DESCONTADA =
                                           dud.ID_DUPLICATA_DESCONTADA)
                                 JOIN empresa_rede emp
                                    ON (dup.id_empresa = emp.id_empresa
                                        AND emp.id_rede_hotel = $P{IDRH})
                                 JOIN conta_corrente cco
                                    ON (cco.ID_conta_corrente = dup.ID_conta_corrente
                                        AND cco.id_hotel = dup.id_hotel)
                                 JOIN banco ban
                                    ON (ban.id_banco = cco.id_banco)
                           WHERE TRUNC (dup.data_desconto) =
                                    TO_DATE ($P{P_DATA}, 'dd/mm/yyyy')
                                 AND dud.id_hotel = $P{IDH})) tb
           WHERE total_mes.id_hotel = tb.id_hotel(+)
          UNION ALL
          SELECT *
            FROM (SELECT 3 ID,
                         $P{IDH} id_hotel,
                         TOTAL_MES.TOTAL TOTAL_MES,
                         TOTAL_MES.JUROS JUROS_MES,
                         TOTAL_MES.DESCONTO DESCONTO_MES,
                         TOTAL_MES.IMPOSTOS IMPOSTOS_MES,
                           TOTAL_MES.TOTAL
                         - TOTAL_MES.JUROS
                         + TOTAL_MES.DESCONTO
                         + TOTAL_MES.IMPOSTOS
                            TOTAL_LIQUIDO_MES
                    FROM (SELECT SUM (NVL (VALOR_RECEBIDO, 0)) TOTAL,
                                 SUM (NVL (JUROS_RECEBIMENTO, 0)) JUROS,
                                 SUM (NVL (DESCONTO_RECEBIMENTO, 0)) DESCONTO,
                                 SUM (
                                      NVL (ISS, 0)
                                    + NVL (COFINS, 0)
                                    + NVL (PIS, 0)
                                    + NVL (IR_RETENCAO, 0)
                                    + NVL (CSSL, 0))
                                    IMPOSTOS
                            FROM DUPLICATA
                           WHERE TRUNC (DATA_RECOMPRA) BETWEEN TRUNC (
                                                                  TO_DATE (
                                                                     $P{P_DATA},
                                                                     'dd/mm/yyyy'),
                                                                  'MONTH')
                                                           AND TO_DATE (
                                                                  $P{P_DATA},
                                                                  'dd/mm/yyyy')
                                 AND ID_HOTEL = $P{IDH}) TOTAL_MES) TOTAL_MES,
                 (SELECT id_hotel,
                         TIPO,
                         VALOR_LIQUIDO,
                         VALOR_IMPOSTO,
                         BANCOCC,
                         EMPRESA,
                         DUP,
                         VALOR_RECEBIDO,
                         NULL VALOR_LIQUIDO_TOTAL,
                         VALOR_DUPLICATA_CALCULADO,
                         JUROS_RECEBIMENTO,
                         DESCONTO_RECEBIMENTO,
                         NUM_CONTA_CORRENTE,
                         ID_BANCO
                    FROM (SELECT 'Recompradas' TIPO,
                                 dup.id_hotel,
                                 (dup.id_duplicata),
                                    dup.num_duplicata
                                 || '-'
                                 || dup.num_parcelas
                                 || '-'
                                 || dup.ano
                                    AS dup,
                                 dup.id_duplicata_descontada,
                                 TRUNC (dup.data_recebimento),
                                 TRUNC (dup.data_recompra),
                                 NVL (dup.valor_recebido, 0) valor_recebido,
                                 NVL (dup.desp_financeira, 0) valor_imposto,
                                 NVL (dup.juros_recebimento, 0) valor_juros,
                                 NVL (dup.ajustes, 0) valor_desconto,
                                 emp.nome_fantasia empresa,
                                 dup.valor_duplicata,
                                   DUP.VALOR_DUPLICATA
                                 - NVL (DUP.ENCARGOS, 0)
                                 - NVL (DUP.COMISSAO, 0)
                                 + NVL (DUP.IR, 0)
                                 - NVL (DUP.AJUSTES, 0)
                                 - NVL (DUP.PIS, 0)
                                 - NVL (DUP.COFINS, 0)
                                 - NVL (DUP.ISS, 0)
                                 - NVL (DUP.IR_RETENCAO, 0)
                                 - NVL (DUP.CSSL, 0)
                                    AS VALOR_LIQUIDO,
                                   NVL (DUP.VALOR_DUPLICATA, 0)
                                 - NVL (DUP.COMISSAO, 0)
                                 - NVL (DUP.AJUSTES, 0)
                                 + NVL (DUP.IR, 0)
                                 - NVL (DUP.ENCARGOS, 0)
                                    VALOR_DUPLICATA_CALCULADO,
                                 TRUNC (dup.data_vencimento),
                                 TRUNC (dup.data_desconto),
                                 (   BAN.NOME_FANTASIA
                                  || '-'
                                  || CCO.NUM_CONTA_CORRENTE)
                                    AS BANCOCC,
                                 DUP.JUROS_RECEBIMENTO,
                                 DUP.DESCONTO_RECEBIMENTO,
                                 CCO.NUM_CONTA_CORRENTE,
                                 CCO.ID_BANCO
                            FROM duplicata dup
                                 JOIN empresa_rede emp
                                    ON (dup.id_empresa = emp.id_empresa
                                        AND emp.id_rede_hotel = $P{IDRH})
                                 JOIN conta_corrente cco
                                    ON (cco.ID_conta_corrente = dup.ID_conta_corrente
                                        AND cco.id_hotel = dup.id_hotel)
                                 JOIN banco ban
                                    ON (ban.id_banco = cco.id_banco)
                           WHERE TRUNC (dup.data_recompra) =
                                    TO_DATE ($P{P_DATA}, 'dd/mm/yyyy')
                                 AND dup.id_hotel = $P{IDH})) tb
           WHERE total_mes.id_hotel = tb.id_hotel(+)),
                    (SELECT NVL (SALDODIA, 0) FAT_DIA, NVL (SALDOMES, 0) FAT_MES
            FROM (SELECT (SUM (
                               (NVL (DU.VALOR_DUPLICATA, 0))
                             - (NVL (DU.ENCARGOS, 0))
                             - (NVL (DU.COMISSAO, 0))
                             + (NVL (DU.IR, 0))
                             - (NVL (DU.AJUSTES, 0))))
                            AS SALDODIA
                    FROM DUPLICATA DU, CONTROLA_DATA CD
                   WHERE DU.ID_HOTEL = $P{IDH} AND DU.ID_HOTEL = CD.ID_HOTEL
                         AND TRUNC (du.DATA_LANCAMENTO) =
                                TO_DATE ($P{P_DATA}, 'dd/mm/yyyy')) FAT_DIA,
                 (SELECT (SUM (
                               (NVL (DU.VALOR_DUPLICATA, 0))
                             - (NVL (DU.ENCARGOS, 0))
                             - (NVL (DU.COMISSAO, 0))
                             + (NVL (DU.IR, 0))
                             - (NVL (DU.AJUSTES, 0))))
                            AS SALDOMES
                    FROM DUPLICATA DU, CONTROLA_DATA CD
                   WHERE DU.ID_HOTEL = $P{IDH} AND DU.ID_HOTEL = CD.ID_HOTEL
                         AND TRUNC (du.DATA_LANCAMENTO) BETWEEN TRUNC (
                                                                   TO_DATE (
                                                                      $P{P_DATA},
                                                                      'dd/mm/yyyy'),
                                                                   'MONTH')
                                                            AND TO_DATE (
                                                                   $P{P_DATA},
                                                                   'dd/mm/yyyy')) FAT_MES) FAT,
         (SELECT SALDO_ANTERIOR_CONTAS_RECEBER
            FROM SALDOS_DUPLICATA SD
           WHERE SD.ANO =
                    TO_CHAR (ADD_MONTHS (TO_DATE ($P{P_DATA}, 'dd/mm/yyyy'), -1),
                             'YYYY')
                 AND SD.MES =
                        TO_CHAR (
                           ADD_MONTHS (TO_DATE ($P{P_DATA}, 'dd/mm/yyyy'), -1),
                           'MM')
                 AND SD.ID_HOTEL = $P{IDH})
ORDER BY ID,
         TIPO,
         BANCOCC,
         NOME_FANTASIA]]>
	</queryString>
	<field name="GERAL" class="java.math.BigDecimal"/>
	<field name="ID" class="java.math.BigDecimal"/>
	<field name="TIPO" class="java.lang.String"/>
	<field name="VALOR_LIQUIDO" class="java.math.BigDecimal"/>
	<field name="IMPOSTOS" class="java.math.BigDecimal"/>
	<field name="BANCOCC" class="java.lang.String"/>
	<field name="NOME_FANTASIA" class="java.lang.String"/>
	<field name="DUP" class="java.lang.String"/>
	<field name="VALREC" class="java.math.BigDecimal"/>
	<field name="SALDO_ANTERIOR_CONTAS_RECEBER" class="java.math.BigDecimal"/>
	<field name="VALOR_LIQUIDO_TOTAL" class="java.math.BigDecimal"/>
	<field name="VALOR_DUPLICATA_CALCULADO" class="java.math.BigDecimal"/>
	<field name="JUROS_RECEBIMENTO" class="java.math.BigDecimal"/>
	<field name="DESCONTO_RECEBIMENTO" class="java.math.BigDecimal"/>
