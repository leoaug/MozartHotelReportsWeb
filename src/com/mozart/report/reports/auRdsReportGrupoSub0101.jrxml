<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="rdsGroupReport1" language="groovy" pageWidth="595" pageHeight="842" columnWidth="572" leftMargin="0" rightMargin="23" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.4641000000000006"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="IDH" class="java.lang.String"/>
	<parameter name="P_DATA" class="java.lang.String"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT * FROM (
SELECT UNIQUE 1 TIPO, TL.GRUPO_LANCAMENTO,TL.SUB_GRUPO_LANCAMENTO ,TL.DESCRICAO_LANCAMENTO, A.RECEITA_CHECKOUT, B.RECEITA_CHECKOUT RECEITA_CHECKOUT_B,
DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, 'S', 'N') GRUPO_DIARIA, A.DIARIA - VALOR_CAFE_DIA AS DIARIA, B.DIARIA_ACUMULADO - VALOR_CAFE_ACM AS DIARIA_ACUMULADO, C.HOSPEDE, C.CRIANCAS, C.CAFE, C.CORTESIA,
D.HOSPEDE_ACUMULADO, D.CRIANCAS_ACUMULADO, D.CAFE_ACUMULADO,D.CORTESIA_ACUMULADO,
E.SCORTESIA, F.SCORTESIA_ACUMULADO,
C.GRUPO,MAPAFAP.MAP,MAPAFAP.FAP,MAPAFAP.ALL_INCLUSIVE,ACU_AMAPAFAP.AMAP,ACU_AMAPAFAP.AFAP,ACU_AMAPAFAP.AALL,
(C.OCUPADOS) OCUPADOS,
(D.OCUPADOS_acumulado) OCUPADOS_ACUMULADO,

D.GRUPO_ACUMULADO, C.DIARIA_GRUPO, D.DIARIA_GRUPO_ACUMULADO,

(DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, A.DIARIA, 0))DIARIA_TOTAL,
(DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, B.DIARIA_ACUMULADO, 0))DIARIA_TOTAL_ACUMULADO,
J.RECEITA_TOTAL, L.RECEITA_TOTAL_ACUMULADO,
DECODE(C.OCUPADOS, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, J.RECEITA_TOTAL, 0)/decode((C.OCUPADOS),0,1,(C.OCUPADOS))))DM_RECEITA_TOTAL,
DECODE(C.OCUPADOS, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, L.RECEITA_TOTAL_ACUMULADO, 0)/decode((C.OCUPADOS),0,1,(C.OCUPADOS))) )DM_RECEITA_TOTAL_ACUMULADO,

(C.OCUPADOS - C.GRUPO) INDIVIDUAL,
(D.OCUPADOS_acumulado - D.GRUPO_ACUMULADO) INDIVIDUAL_ACUMULADO,

(A.DIARIA - C.DIARIA_GRUPO )DIARIA_INDIVIDUAL, (B.DIARIA_ACUMULADO - D.DIARIA_GRUPO_ACUMULADO)DIARIA_INDIV_ACUMULADO,

NVL(G.APARTAMENTO_DIA,0) APARTAMENTO_DIA,
NVL(H.APARTAMENTO_ACUMULADO,0) APARTAMENTO_ACUMULADO,

((DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, A.DIARIA, 0))/NVL(G.APARTAMENTO_DIA,0)) REVPAR_OCUPADOS_DIA,
((DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, B.DIARIA_ACUMULADO, 0))/NVL(H.APARTAMENTO_ACUMULADO,0) )REVPAR_OCUPADOS_ACUMULADO,

DECODE(C.OCUPADOS, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, A.DIARIA, 0)/(DECODE(C.OCUPADOS,0,1,C.OCUPADOS))) )DM_OCUPADOS_DIA,
DECODE((D.OCUPADOS_acumulado), 0, 0,(DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, B.DIARIA_ACUMULADO, 0)/(D.OCUPADOS_acumulado)) )DM_OCUPADOS_ACUMULADO,

DECODE(E.SCORTESIA, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, A.DIARIA, 0)/DECODE(E.SCORTESIA,0,1,E.SCORTESIA)) )DM_SCORTESIA_DIA,
DECODE(F.SCORTESIA_ACUMULADO, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, B.DIARIA_ACUMULADO, 0)/F.SCORTESIA_ACUMULADO) )DM_SCORTESIA_ACUMULADO,
DECODE(C.CORTESIA, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, A.DIARIA, 0)/C.CORTESIA) )DM_CORTESIA_DIA,
DECODE(D.CORTESIA_ACUMULADO, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, B.DIARIA_ACUMULADO, 0)/D.CORTESIA_ACUMULADO) )DM_CORTESIA_ACUMULADO,
DECODE(C.HOSPEDE, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO,1, A.DIARIA, 0)/C.HOSPEDE) )DM_HOSPEDE_DIA,
DECODE(D.HOSPEDE_ACUMULADO, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, B.DIARIA_ACUMULADO, 0)/D.HOSPEDE_ACUMULADO) )DM_HOSPEDE_ACUMULADO,
DECODE(C.GRUPO, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, C.DIARIA_GRUPO, 0)/C.GRUPO) )DM_GRUPO_DIA,
DECODE(D.GRUPO_ACUMULADO, 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, D.DIARIA_GRUPO_ACUMULADO, 0)/D.GRUPO_ACUMULADO) )DM_GRUPO_ACUMULADO,

DECODE((C.OCUPADOS- C.GRUPO), 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, (A.DIARIA - C.DIARIA_GRUPO ), 0)/(DECODE(C.OCUPADOS - C.GRUPO,0,1,C.OCUPADOS - C.GRUPO))) )DM_INDIVIDUAL_DIA,
DECODE((D.OCUPADOS_acumulado - D.GRUPO_ACUMULADO), 0, 0, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, (B.DIARIA_ACUMULADO - D.DIARIA_GRUPO_ACUMULADO), 0)/(D.OCUPADOS_acumulado - D.GRUPO_ACUMULADO)))DM_INDIVIDUAL_ACUMULADO,

((C.OCUPADOS)*100/decode(nvl(G.APARTAMENTO_DIA,0),0,100,G.APARTAMENTO_DIA)) PERCENTUAL_OCUPADOS,

((D.OCUPADOS_acumulado)*100/decode(nvl(H.APARTAMENTO_ACUMULADO,0),0,100,H.APARTAMENTO_ACUMULADO)) PERCENTUAL_OCUPADOS_ACUMULADO,

((C.OCUPADOS - C.GRUPO)*100/decode(nvl(G.APARTAMENTO_DIA,0),0,100,G.APARTAMENTO_DIA)) PERCENTUAL_INDIV,

((D.OCUPADOS_acumulado - D.GRUPO_ACUMULADO)*100/decode(nvl(H.APARTAMENTO_ACUMULADO,0),0,100,H.APARTAMENTO_ACUMULADO)) PERCENTUAL_INDIV_ACUMULADO,

( C.GRUPO*100/decode(nvl(G.APARTAMENTO_DIA,0),0,100,G.APARTAMENTO_DIA)) PERCENTUAL_GRUPO,

(D.GRUPO_ACUMULADO*100/decode(nvl(H.APARTAMENTO_ACUMULADO,0),0,100,H.APARTAMENTO_ACUMULADO)) PERCENTUAL_GRUPO_ACUMULADO,

( C.CORTESIA*100/decode(nvl(G.APARTAMENTO_DIA,0),0,100,G.APARTAMENTO_DIA)) PERCENTUAL_CORTESIA,

(D.CORTESIA_ACUMULADO*100/decode(nvl(H.APARTAMENTO_ACUMULADO,0),0,100,H.APARTAMENTO_ACUMULADO)) PERCENTUAL_CORTESIA_ACUMULADO,

(E.SCORTESIA*100/decode(nvl(G.APARTAMENTO_DIA,0),0,100,G.APARTAMENTO_DIA)) PERCENTUAL_SCORTESIA,

(F.SCORTESIA_ACUMULADO*100/decode(nvl(H.APARTAMENTO_ACUMULADO,0),0,100,H.APARTAMENTO_ACUMULADO)) PERCENTUAL_SCORTESIA_ACUMULADO,

I.DOLAR,

C.VALOR_CAFE_DIA CAFEDIA, D.VALOR_CAFE_ACM CAFEMES, (DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, A.DIARIA, 0)  - (C.VALOR_CAFE_DIA)) RECEITA_LIQUIDA_DIA,

(DECODE(TL.ID_IDENTIFICA_LANCAMENTO, 1, B.DIARIA_ACUMULADO,0) - (D.VALOR_CAFE_ACM)) RECEITA_LIQUIDA_MES
,
 pensao.pensoes

------


FROM TIPO_LANCAMENTO TL,

--Tabela
(SELECT NVL(SUM(MAP),0) MAP,NVL(SUM(FAP),0) FAP, NVL(SUM(ALL_INCLUSIVE),0) ALL_INCLUSIVE
FROM MOVIMENTO_APARTAMENTO
WHERE ID_HOTEL = $P{IDH}
AND trunc(DATA_LANCAMENTO) = to_date($P{P_DATA},'dd/mm/yyyy') )MAPAFAP,


--TaBela
(SELECT NVL(SUM(MAP),0) AMAP,NVL(SUM(FAP),0) AFAP, NVL(SUM(ALL_INCLUSIVE),0) AALL
FROM MOVIMENTO_APARTAMENTO
WHERE ID_HOTEL = $P{IDH}
AND trunc(DATA_LANCAMENTO) BETWEEN TRUNC(to_date($P{P_DATA},'dd/mm/yyyy'), 'MONTH') AND to_date($P{P_DATA},'dd/mm/yyyy') )ACU_AMAPAFAP,

--Tabela

((SELECT ID_LANCAMENTO_PAI ID_DIARIA, RECEITA_CHECKOUT, NVL(ABS(SUM(MA.VALOR_LANCAMENTO)), 0) DIARIA
FROM   MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, IDENTIFICA_LANCAMENTO IL
WHERE  TL.ID_LANCAMENTO_PAI IS NOT NULL
AND trunc(MA.DATA_LANCAMENTO(+)) = to_date($P{P_DATA},'dd/mm/yyyy')
AND TL.ID_TIPO_LANCAMENTO = MA.ID_TIPO_LANCAMENTO(+)
AND IL.ID_IDENTIFICA_LANCAMENTO IN (2,3,26,28)
AND TL.ID_HOTEL = $P{IDH}
AND MA.ID_HOTEL = TL.ID_HOTEL
AND TL.ID_IDENTIFICA_LANCAMENTO = IL.ID_IDENTIFICA_LANCAMENTO
AND IL.RECEITA_CHECKOUT = 1 -- EXCETO PAGAMENTO
GROUP BY ID_LANCAMENTO_PAI, RECEITA_CHECKOUT) ) A,

--TABELA

((SELECT ID_LANCAMENTO_PAI ID_DIARIA_ACUMULADO, RECEITA_CHECKOUT, NVL(ABS(SUM(MA.VALOR_LANCAMENTO)), 0) DIARIA_ACUMULADO

FROM   MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, IDENTIFICA_LANCAMENTO IL
WHERE  TL.ID_LANCAMENTO_PAI IS NOT NULL

AND trunc(MA.DATA_LANCAMENTO(+)) BETWEEN TRUNC(to_date($P{P_DATA},'dd/mm/yyyy'), 'MONTH') AND to_date($P{P_DATA},'dd/mm/yyyy')
AND TL.ID_TIPO_LANCAMENTO = MA.ID_TIPO_LANCAMENTO(+)
AND IL.ID_IDENTIFICA_LANCAMENTO IN (2,3,26,28)
AND TL.ID_HOTEL = $P{IDH}
AND MA.ID_HOTEL = TL.ID_HOTEL

AND TL.ID_IDENTIFICA_LANCAMENTO = IL.ID_IDENTIFICA_LANCAMENTO
AND IL.RECEITA_CHECKOUT = 1 -- EXCETO PAGAMENTO
GROUP BY ID_LANCAMENTO_PAI, RECEITA_CHECKOUT) ) B,


--Tabela
(
SELECT

COUNT(DUAL.DUMMY), MA.RECEITA_DIARIA, MA.OCUPADOS, MA.HOSPEDE,
C.GRUPO, MA.CRIANCAS, MA.CAFES CAFE,B.CORTESIA, C.DIARIA_GRUPO, MA.VALOR_CAFE_DIA
FROM DUAL,

(

SELECT
SUM(NVL(MA.VALOR_LANCAMENTO,0)) RECEITA_DIARIA,
COUNT((DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,NULL,DECODE(TL.ID_IDENTIFICA_LANCAMENTO,28,NULL, MA.ID_MOVIMENTO_APARTAMENTO))))
-
COUNT((DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,MA.ID_MOVIMENTO_APARTAMENTO,NULL)))OCUPADOS,

NVL(SUM(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,-MA.QTDE_ADULTOS,MA.QTDE_ADULTOS)),0) HOSPEDE,

NVL(SUM(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,-MA.QTDE_CRIANCAS,MA.QTDE_CRIANCAS)),0) CRIANCAS,

NVL(SUM(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,-MA.QTDE_CAFE,MA.QTDE_CAFE)),0) CAFES,

NVL(SUM(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,-MA.QTDE_CAFE,MA.QTDE_CAFE) * VC.VALOR_CAFE ),0)  VALOR_CAFE_DIA

FROM MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, CHECKIN C,
(select id_hotel, data, valor_cafe, d2
from valor_cafe vc, tipo_pensao tp
where vc.id_tipo_pensao = tp.id_tipo_pensao and id_hotel = $P{IDH} and data = to_date($P{P_DATA},'dd/mm/yyyy')  and tp.ID_TIPO_PENSAO = 1) vc

WHERE MA.ID_CHECKIN = C.ID_CHECKIN
AND MA.ID_HOTEL = C.ID_HOTEL
--AND trunc(MA.DATA_LANCAMENTO) BETWEEN trunc(C.DATA_ENTRADA) AND trunc(C.DATA_SAIDA)
AND trunc(MA.DATA_LANCAMENTO) = to_date($P{P_DATA},'dd/mm/yyyy')
AND MA.ID_HOTEL = $P{IDH}
AND MA.ID_TIPO_LANCAMENTO = TL.ID_TIPO_LANCAMENTO
AND TL.ID_HOTEL = MA.ID_HOTEL
AND TL.ID_IDENTIFICA_LANCAMENTO IN (26, 2, 3,28)

) MA,



(SELECT COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,NULL,MA.ID_CHECKIN))
-
COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,MA.ID_CHECKIN,NULL)) CORTESIA
FROM CHECKIN C, MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, CONTROLA_DATA H
WHERE TL.ID_IDENTIFICA_LANCAMENTO IN (2, 26,3)
AND trunc(MA.DATA_LANCAMENTO) = to_date($P{P_DATA},'dd/mm/yyyy')
AND MA.ID_HOTEL = $P{IDH}
AND MA.ID_HOTEL = H.ID_HOTEL
AND MA.ID_TIPO_LANCAMENTO = TL.ID_TIPO_LANCAMENTO
AND C.CORTESIA  = 'S'
AND C.ID_HOTEL = MA.ID_HOTEL
AND C.ID_CHECKIN = MA.ID_CHECKIN

) B,


(SELECT
COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,NULL,DECODE(TL.ID_IDENTIFICA_LANCAMENTO,28,NULL,MA.ID_CHECKIN)))
-
COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,MA.ID_CHECKIN,NULL)) GRUPO,
NVL(ABS(SUM(MA.VALOR_LANCAMENTO)), 0) DIARIA_GRUPO
FROM MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, CHECKIN CK, RESERVA R
WHERE TL.ID_IDENTIFICA_LANCAMENTO IN (26, 2, 3,28)
AND trunc(MA.DATA_LANCAMENTO) = to_date($P{P_DATA},'dd/mm/yyyy')
AND MA.ID_HOTEL =$P{IDH}
AND ck.ID_HOTEL =$P{IDH}
AND MA.ID_TIPO_LANCAMENTO = TL.ID_TIPO_LANCAMENTO
AND MA.ID_CHECKIN = CK.ID_CHECKIN
AND CK.ID_RESERVA = R.ID_RESERVA AND R.GRUPO='S') C

GROUP BY MA.RECEITA_DIARIA,MA.OCUPADOS,MA.HOSPEDE,MA.CRIANCAS,B.CORTESIA, C.GRUPO, C.DIARIA_GRUPO, MA.CAFES, MA.VALOR_CAFE_DIA
) C,

--Tabela
(SELECT
COUNT(DUAL.DUMMY),
MA.RECEITA_DIARIA,
MA.OCUPADOS OCUPADOS_ACUMULADO,
MA.HOSPEDE HOSPEDE_ACUMULADO,
C.GRUPO GRUPO_ACUMULADO,
MA.CRIANCAS CRIANCAS_ACUMULADO,
MA.CAFES CAFE_ACUMULADO,
B.CORTESIA CORTESIA_ACUMULADO,
C.DIARIA_GRUPO DIARIA_GRUPO_ACUMULADO,
MA.VALOR_CAFE_ACM
FROM DUAL,

(SELECT
/*+INDEX( MOVIMENTO_APARTAMENTO IDX_MOV_APTO_LANC )*/
SUM(NVL(MA.VALOR_LANCAMENTO,0)) RECEITA_DIARIA,

COUNT((DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,NULL,DECODE(TL.ID_IDENTIFICA_LANCAMENTO,28,NULL, MA.ID_MOVIMENTO_APARTAMENTO))))
-
COUNT((DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,MA.ID_MOVIMENTO_APARTAMENTO,NULL)))OCUPADOS,

NVL(SUM(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,-MA.QTDE_ADULTOS,MA.QTDE_ADULTOS)),0) HOSPEDE,

NVL(SUM(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,-MA.QTDE_CRIANCAS,MA.QTDE_CRIANCAS)),0) CRIANCAS,

NVL(SUM(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,-MA.QTDE_CAFE,MA.QTDE_CAFE) ),0) CAFES,

NVL(SUM(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,-MA.QTDE_CAFE,MA.QTDE_CAFE) * VC.VALOR_CAFE ),0)  VALOR_CAFE_ACM

FROM MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, CHECKIN C,

(select id_hotel, data, valor_cafe, d2
from valor_cafe vc, tipo_pensao tp
where vc.id_tipo_pensao = tp.id_tipo_pensao and id_hotel = $P{IDH} and data = to_date($P{P_DATA},'dd/mm/yyyy') and tp.ID_TIPO_PENSAO = 1) vc

WHERE MA.ID_CHECKIN = C.ID_CHECKIN
AND MA.ID_HOTEL = C.ID_HOTEL
--AND trunc(MA.DATA_LANCAMENTO) BETWEEN trunc(C.DATA_ENTRADA) AND trunc(C.DATA_SAIDA)
AND MA.ID_HOTEL = $P{IDH}
AND MA.ID_TIPO_LANCAMENTO = TL.ID_TIPO_LANCAMENTO
AND TL.ID_HOTEL = MA.ID_HOTEL
AND trunc(MA.DATA_LANCAMENTO) BETWEEN TRUNC(to_date($P{P_DATA},'dd/mm/yyyy'), 'MONTH') AND to_date($P{P_DATA},'dd/mm/yyyy')
AND TL.ID_IDENTIFICA_LANCAMENTO IN (26, 2, 3,28)
) MA,


(SELECT COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,NULL,MA.ID_CHECKIN))
-
COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,MA.ID_CHECKIN,NULL)) CORTESIA
FROM CHECKIN C, MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, CONTROLA_DATA H
WHERE TL.ID_IDENTIFICA_LANCAMENTO IN (2, 26,3)
AND trunc(MA.DATA_LANCAMENTO) BETWEEN TRUNC(to_date($P{P_DATA},'dd/mm/yyyy'), 'MONTH') AND to_date($P{P_DATA},'dd/mm/yyyy')
AND MA.ID_HOTEL = $P{IDH}
AND MA.ID_HOTEL = H.ID_HOTEL
AND MA.ID_TIPO_LANCAMENTO = TL.ID_TIPO_LANCAMENTO
AND C.CORTESIA  = 'S'
AND C.ID_HOTEL = MA.ID_HOTEL
AND C.ID_CHECKIN = MA.ID_CHECKIN


) B,


(SELECT

COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,NULL,DECODE(TL.ID_IDENTIFICA_LANCAMENTO,28,NULL,MA.ID_CHECKIN)))
-
COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,MA.ID_CHECKIN,NULL)) GRUPO,
NVL(ABS(SUM(MA.VALOR_LANCAMENTO)), 0) DIARIA_GRUPO
FROM MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, CHECKIN CK, RESERVA R
WHERE TL.ID_IDENTIFICA_LANCAMENTO IN (26, 2, 3,28)
AND trunc(MA.DATA_LANCAMENTO) BETWEEN TRUNC(to_date($P{P_DATA},'dd/mm/yyyy'), 'MONTH') AND to_date($P{P_DATA},'dd/mm/yyyy')
AND MA.ID_HOTEL = $P{IDH}
AND CK.ID_HOTEL = $P{IDH}
AND MA.ID_TIPO_LANCAMENTO = TL.ID_TIPO_LANCAMENTO
AND MA.ID_CHECKIN = CK.ID_CHECKIN
AND CK.ID_RESERVA = R.ID_RESERVA AND R.GRUPO='S') C
GROUP BY

MA.RECEITA_DIARIA,MA.OCUPADOS,MA.HOSPEDE,MA.CRIANCAS,B.CORTESIA, C.GRUPO, C.DIARIA_GRUPO, MA.CAFES, MA.VALOR_CAFE_ACM
) D,


-- O ERRO ESTA AQUI,,, QDO FOR PRA DIVIDIR E FOR 0 COLOCAR O RESULTADO FINAL 0.. CONSULTA ABAIXO É USADA EM ALGUM CALCULO
--Tabela
(SELECT

COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,NULL,MA.ID_CHECKIN))
-

COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,MA.ID_CHECKIN,NULL))
SCORTESIA
FROM CHECKIN C, MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL
WHERE trunc(MA.DATA_LANCAMENTO) = to_date($P{P_DATA},'dd/mm/yyyy')
AND MA.ID_HOTEL = $P{IDH}
AND MA.ID_TIPO_LANCAMENTO = TL.ID_TIPO_LANCAMENTO
AND TL.ID_HOTEL = MA.ID_HOTEL
AND TL.ID_IDENTIFICA_LANCAMENTO IN (2, 26, 3)
AND C.CORTESIA = 'N'
AND C.ID_HOTEL = MA.ID_HOTEL
AND C.ID_CHECKIN = MA.ID_CHECKIN


) E,

--Tabela

(
SELECT MA.VALOR SCORTESIA_ACUMULADO
FROM

(SELECT COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,NULL,MA.ID_MOVIMENTO_APARTAMENTO))
-
COUNT(DECODE(TL.ID_IDENTIFICA_LANCAMENTO,3,MA.ID_MOVIMENTO_APARTAMENTO,NULL))VALOR
FROM CHECKIN C, MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL

WHERE trunc(MA.DATA_LANCAMENTO) BETWEEN  TRUNC(to_date($P{P_DATA},'dd/mm/yyyy'), 'MONTH')AND to_date($P{P_DATA},'dd/mm/yyyy')
AND MA.ID_HOTEL = $P{IDH}
AND C.ID_HOTEL = $P{IDH}
AND MA.ID_TIPO_LANCAMENTO = TL.ID_TIPO_LANCAMENTO
AND TL.ID_IDENTIFICA_LANCAMENTO IN (2, 26, 3)
AND C.CORTESIA  = 'N'
AND C.ID_HOTEL = MA.ID_HOTEL
AND C.ID_CHECKIN = MA.ID_CHECKIN


) MA
) F,
--Tabela

(
SELECT COUNT(*) APARTAMENTO_DIA
FROM APARTAMENTO A
WHERE A.ID_HOTEL = $P{IDH}
AND COFAN = 'N'
)
G,
--Tabela
(SELECT (COUNT(*) * TO_CHAR(to_date($P{P_DATA},'dd/mm/yyyy'), 'DD'))  APARTAMENTO_ACUMULADO
FROM APARTAMENTO A
WHERE A.ID_HOTEL = $P{IDH}
AND COFAN = 'N'
) H,
--Tabela
(SELECT VALOR_DOLAR DOLAR
FROM VALOR_DOLAR V,

(SELECT MAX(DATA) DATA, ID_HOTEL
FROM VALOR_DOLAR
WHERE trunc(DATA) <= to_date($P{P_DATA},'dd/mm/yyyy')
AND ID_HOTEL =$P{IDH}
AND ID_MOEDA = 2
GROUP BY ID_HOTEL) D
WHERE D.DATA = V.DATA
AND ID_MOEDA = 2
AND V.ID_HOTEL = D.ID_HOTEL) I,
--Tabela

(SELECT  ROUND(NVL(ABS(SUM(MA.VALOR_LANCAMENTO)), 0), 2) RECEITA_TOTAL
FROM   MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, IDENTIFICA_LANCAMENTO IL
WHERE TL.ID_HOTEL = $P{IDH}
AND IL.RECEITA_CHECKOUT = 1
AND trunc(MA.DATA_LANCAMENTO(+)) = to_date($P{P_DATA},'dd/mm/yyyy')
AND TL.ID_TIPO_LANCAMENTO = MA.ID_TIPO_LANCAMENTO(+)
AND MA.ID_HOTEL = TL.ID_HOTEL
AND TL.ID_IDENTIFICA_LANCAMENTO = IL.ID_IDENTIFICA_LANCAMENTO
) J,
--Tabela

(SELECT  ROUND(NVL(ABS(SUM(MA.VALOR_LANCAMENTO)), 0), 2) RECEITA_TOTAL_ACUMULADO

FROM   MOVIMENTO_APARTAMENTO MA, TIPO_LANCAMENTO TL, IDENTIFICA_LANCAMENTO IL
WHERE TL.ID_HOTEL = $P{IDH}
AND IL.RECEITA_CHECKOUT = 1
AND trunc(MA.DATA_LANCAMENTO(+)) BETWEEN TRUNC(to_date($P{P_DATA},'dd/mm/yyyy'), 'MONTH')AND to_date($P{P_DATA},'dd/mm/yyyy')
AND TL.ID_TIPO_LANCAMENTO = MA.ID_TIPO_LANCAMENTO(+)
AND MA.ID_HOTEL = TL.ID_HOTEL
AND TL.ID_IDENTIFICA_LANCAMENTO = IL.ID_IDENTIFICA_LANCAMENTO
) L,


(select valor_cafe PENSOES from valor_cafe where id_hotel = $P{IDH} and trunc(data) = to_date($P{P_DATA},'dd/mm/yyyy') ) pensao


WHERE A.ID_DIARIA(+)      = TL.ID_TIPO_LANCAMENTO
AND B.ID_DIARIA_ACUMULADO(+)  = TL.ID_TIPO_LANCAMENTO
AND TL.ID_HOTEL = $P{IDH}


ORDER BY B.RECEITA_CHECKOUT, TL.GRUPO_LANCAMENTO, TL.SUB_GRUPO_LANCAMENTO)WHERE ROWNUM = 1]]>
	</queryString>
